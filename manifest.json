{
    "manifest_version": 3,
    "name": "get free games",
    "version": "1.0",
    "action": {
        "default_popup": "hello.html"
    },
    "description": "automate getting free games from EpicGames Store",
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["https://store.epicgames.com/en-US/", "https://store.epicgames.com/en-US/p/*"]
        }
    ]
}
